{
  "name": "free-font",
  "private": false,
  "version": "2.20.0",
  "type": "module",
  "homepage": "https://jaywcjlove.github.io/free-font/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/free-font"
  },
  "scripts": {
    "all": "node scripts/main.mjs && npm run start",
    "one": "node scripts/main.mjs -a",
    "build:docker": "ejsc \"templates/**/*\" -o \"docs\" --global-data '{\"docker\": true}'",
    "build:docker:pages": "node scripts/main.mjs -pages -docker",
    "start": "ejsc \"templates/**/*\" -o \"docs\" && npm run start:pages",
    "start:pages": "node scripts/main.mjs -pages",
    "dev": "ejsc \"templates/**/*\" -o \"docs\" -w"
  },
  "keywords": [],
