  "license": "MIT",
  "description": "",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@wcj/ejs-cli": "^2.2.0",
    "fontkit": "^2.0.4",
    "fs-extra": "^11.2.0",
    "pretty-bytes": "^6.1.1",
    "puppeteer": "^22.13.1"
  }
}
